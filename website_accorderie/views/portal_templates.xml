<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="layout" name="Main layout" inherit_id="portal.frontend_layout">
        <xpath expr="//div[@id='wrapwrap']" position="attributes">
          <attribute name="ng-app">AccorderieApp</attribute>
          <attribute name="ng-controller">MainController</attribute>
          <attribute name="ng-mousemove">updateMoveMouse($event)</attribute>
        </xpath>

        <xpath expr="//div[@id='wrapwrap']" position="inside">
            <div
                class="modal fade modal_pub_offre modal_shown show ng-cloak"
                style="display:block"
                ng-show="show_qrcode_modal"
            >
                <div class="modal-dialog">
                    <div id="s_media_block_modal" class="modal-content">
                        <button class="btn btn-primary btn-lg" ng-click="show_qrcode_modal = false"><i
                                class="fa fa-times-circle"
                            /> Fermer</button>
                        <div class="modal-body">
                            <h2>Partager la page!</h2>
                        </div>
                        <div id="qrcode" />
                    </div>
                </div>
            </div>
            <div
                class="modal fade modal_pub_offre modal_shown show ng-cloak"
                style="display:block"
                ng-show="show_camera_qrcode_modal"
            >
                <div class="modal-dialog">
                    <div id="s_media_block_modal" class="modal-content">
                        <button class="btn btn-primary btn-lg" ng-click="show_camera_close()"><i
                                class="fa fa-times-circle"
                            /> Fermer</button>
                        <div class="modal-body">
                            <h3>Lecteur Code QR</h3>
                            <div ng-if="_.isEmpty(list_camera_qrcode)">
                                Aucune caméra n'est détecté, avez-vous les permissions adéquates?
                            </div>
                            <div ng-if="!_.isEmpty(list_camera_qrcode)">
                                <button
                                    ng-repeat="option in list_camera_qrcode"
                                    class="btn btn-sm"
                                    ng-class="{'btn-primary': option.id === selectedCamera.id}"
                                    ng-click="show_camera_select(option)"
                                >Caméra {{option.label}}
                                </button>
                                <!-- TODO select with ng-model not work, cannot be updated in modal -->
                                <!--<label for="cameraSelect">Liste des caméras : </label>-->
                                <!--<select id="cameraSelect" name="cameraSelect" ng-model="selectedCamera"><option-->
                                <!--        ng-repeat="option in list_camera_qrcode"-->
                                <!--        value="{{option.id}}"-->
                                <!--    >{{option.label}}</option></select>-->
                                <!--<select-->
                                <!--    ng-model="selectedCamera" ng-options="item as item.label for item in list_camera_qrcode" -->
                                <!--&gt; </select>-->
                            </div>
                            <!--<div ng-if="!_.isUndefined(selectedCamera)"><span class="">{{selectedCamera.id}}<br-->
                            <!--        />{{selectedCamera.label}}<br /></span><button-->
                            <!--        class="btn btn-primary btn-lg"-->
                            <!--        ng-click="show_camera_open()"-->
                            <!--    ><i class="fa fa-camera" /> Ouvrir</button></div>-->
                            <div
                                ng-if="show_camera_find_url"
                                style="color:green;"
                            >Lien trouvé, redirection dans 2 secondes vers
                            </div>
                            <div ng-if="!_.isUndefined(show_camera_link_find)">{{show_camera_link_find}}</div>
                            <div style="color:red;" ng-if="!_.isEmpty(show_camera_error)">Erreur : {{show_camera_error}}
                            </div>
                        </div>
                        <div id="reader" />
                    </div>
                </div>
